    "df = pd.DataFrame({'X': [7, 2, 0, 3, 4, 2, 5, 0, 3, 4]})\n",
    "\n",
    "def distance_from_last_zero(col):\n",
    "    zero_found = False\n",
    "    last_zero_index = -1\n",
    "    distances = []\n",
    "    for i in range(len(col)):\n",
    "        if col[i] == 0:\n",
    "            zero_found = True\n",
    "            last_zero_index = i\n",
    "        if not zero_found:\n",
    "            distances.append(i + 1)\n",
    "        else:\n",
    "            distances.append(i - last_zero_index)\n",
    "    return distances\n",
    "\n",
    "df['Y'] = pd.Series(distance_from_last_zero(df['X']))\n",
    "\n",
    "df"
